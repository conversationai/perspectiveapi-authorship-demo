    "@types/gapi": "^0.0.39",
    "@types/gsap": "^1.19.2",
    "@types/lodash": "^4.14.108",
    "@webcomponents/custom-elements": "^1.0.6",
    "codelyzer": "5.1.2",
    "concurrently": "^5.0.0",
